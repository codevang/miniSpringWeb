<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN" 
"http://mybatis.org/dtd/mybatis-3-config.dtd">

<!-- VO 클래스와 동일하게 (Table name + VO) -->
<configuration>
	<typeAliases>
		<!-- 회원 정보 -->
		<typeAlias alias="UserInfoVO" type="hs.spring.hsweb.mapper.vo.user.UserInfoVO" />
		<!-- Remember-me Token -->
		<typeAlias alias="UserRememberMeVO"
			type="hs.spring.hsweb.mapper.vo.user.UserRememberMeVO" />
		<!-- Basic Board -->
		<typeAlias alias="BasicBoardVO" type="hs.spring.hsweb.mapper.vo.board.BasicBoardVO" />
		<!-- 게시판 관리 테이블 -->
		<typeAlias alias="RootBoardVO" type="hs.spring.hsweb.mapper.vo.board.RootBoardVO" />
	</typeAliases>
</configuration>